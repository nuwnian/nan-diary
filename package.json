{
  "name": "nan-diary",
  "version": "1.0.0",
  "description": "A cozy diary app for creative project planning with Firebase integration",
  "type": "module",
  "main": "dashboard.html",
  "scripts": {
    "dev": "vite",
    "dev:fullstack": "concurrently \"npm run dev\" \"npm run dev:backend\"",
    "dev:frontend": "vite",
    "dev:backend": "cd server && npm run dev",
    "build": "tsc && vite build",
    "sentry:sourcemaps": "sentry-cli sourcemaps upload --org=$SENTRY_ORG --project=$SENTRY_PROJECT ./deploy",
    "build:old": "npm run build:prepare && npm run build:copy && npm run build:config && npm run build:inject",
    "build:prepare": "echo 'ðŸ—ï¸  Preparing build...'",
    "build:copy": "node scripts/build/copy.js",
    "build:config": "node scripts/build/build-config.js",
    "build:inject": "node scripts/build/inject-env.js",
    "preview": "vite preview",
    "deploy": "npm run build && firebase deploy --only hosting",
    "deploy:full": "npm run build && firebase deploy",
    "deploy:rules": "firebase deploy --only firestore:rules",
    "lint": "eslint src/js/*.js --fix",
    "lint:check": "eslint src/js/*.js",
    "lint:backend": "cd server && npm run lint",
    "security-check": "npm audit --audit-level moderate",
    "test": "jest --runInBand",
    "test:backend": "cd server && npm test",
    "test:all": "npm test && npm run test:backend",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "test:e2e": "npm run dev & cypress run",
    "dev:e2e": "concurrently \"npm run dev\" \"npm run cypress:open\"",
    "start": "npm run dev:fullstack",
    "postbuild": "echo 'âœ… Build complete! Files ready in deploy/ directory'",
    "setup": "npm install && cd server && npm install",
    "setup:backend": "cd server && npm install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nuwnian/nan-diary.git"
  },
  "keywords": [
    "diary",
    "creative",
    "planning",
    "firebase",
    "pwa",
    "responsive",
    "security"
  ],
  "author": "nuwnian",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nuwnian/nan-diary/issues"
  },
  "homepage": "https://nan-diary-6cdba.web.app",
  "devDependencies": {
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.3",
    "autoprefixer": "^10.4.20",
    "concurrently": "^8.2.2",
    "cypress": "^15.5.0",
    "dotenv": "^17.2.3",
    "eslint": "^8.57.0",
    "firebase-tools": "^13.0.0",
    "jest": "^29.0.0",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.15",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.20.6",
    "typescript": "^5.6.3",
    "vite": "^5.4.11"
  },
  "dependencies": {
    "@radix-ui/react-accordion": "^1.2.2",
    "@radix-ui/react-alert-dialog": "^1.1.3",
    "@radix-ui/react-aspect-ratio": "^1.1.1",
    "@radix-ui/react-avatar": "^1.1.2",
    "@radix-ui/react-checkbox": "^1.1.3",
    "@radix-ui/react-collapsible": "^1.1.2",
    "@radix-ui/react-context-menu": "^2.2.16",
    "@radix-ui/react-dialog": "^1.1.3",
